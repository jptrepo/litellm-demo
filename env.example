# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google AI Studio API Key
GOOGLE_API_KEY=your_google_api_key_here

# Azure OpenAI (if using Azure)
AZURE_API_KEY=your_azure_api_key_here
AZURE_API_BASE=https://your-resource-name.openai.azure.com/

# AWS Credentials (if using Bedrock)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION_NAME=us-east-1

# Generally recommend strong for both LiteLLM Master Key and Salt Key (generate with: openssl rand -hex 32 | sed 's/^/sk-prod-/')
LITELLM_MASTER_KEY=sk-prod-super-user-admin-password!
LITELLM_SALT_KEY=sk-prod-super-secret-salt-key!!!
# Database Configuration
DB_PASSWORD=your_secure_db_password_here


